{
  "name": "node-js-getting-started",
  "version": "1.0.0",
  "private": true,
  "description": "A sample Node.js app using Express",
  "main": "server.js",
  "scripts": {
    "build": "babel public -d lib",
    "start": "node server.js",
    "dev": "npm run watch && nodemon server.js -e js,ejs-- ",
    "watch": "npm run watch-js && npm run watch-css",
    "watch-js": "./node_modules/.bin/watchify src/scripts/app.js --debug --verbose --transform babelify -o lib/scripts/app.js&",
    "watch-css": "./node_modules/.bin/node-sass -w --output-style compressed src/styles/style.scss dist/assets/css/style.css&"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel": "^6.23.0",
    "backbone": "^1.3.3",
    "backbone.paginator": "^2.0.6",
    "body-parser": "1.12.3",
    "browserify": "^14.5.0",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "^1.3.5",
    "coreutils": "^0.3.0",
    "debug": "^3.1.0",
    "ejs": "2.3.1",
    "es6-promise": "^4.1.1",
    "eventemitter3": "^2.0.3",
    "express": "4.12.3",
    "lean": "^0.1.1",
    "leancloud-realtime": "^3.5.7",
    "leancloud-realtime-plugin-live-query": "^1.0.0",
    "leancloud-storage": "^3.3.1",
    "leanengine": "^3.0.0",
    "localstorage-memory": "^1.0.2",
    "md5": "^2.2.1",
    "morgan": "^1.9.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "superagent": "^3.8.1",
    "underscore": "^1.8.3",
    "watchify": "^3.9.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "6.x"
  }
}
